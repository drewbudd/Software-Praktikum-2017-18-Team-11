<?xml version="1.0" encoding="utf-8"?>
<com.sothree.slidinguppanel.SlidingUpPanelLayout
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/sliding_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    sothree:umanoPanelHeight="64dp"
    sothree:umanoShadowHeight="4dp">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="de.uni_stuttgart.informatik.sopra.sopraapp.view.map.MapFragment"
        android:id="@+id/map_fragment"
        android:gravity="center" />

    <fragment
        android:gravity="center|top"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/manageServiceFragment"
        android:name="de.uni_stuttgart.informatik.sopra.sopraapp.view.map.ManageServiceFragment" />

</com.sothree.slidinguppanel.SlidingUpPanelLayout>
